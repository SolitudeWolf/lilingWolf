<script>
    let bottom = document.getElementById('bottom')
    let right = document.getElementById('right')
    let btn1 = document.getElementById('btn1')
    let btn2 = document.getElementById('btn2')
    let bottom_box = document.getElementById('bottom_box')
    let bheight=bottom.style.height;
    bottom.onmouseover = function () {
        bottom.style.cursor = "pointer";
    }
    btn2.style.width = "25px";
    btn2.style.height = "25px";
    btn2.style.cursor = "pointer";
    btn2.style.fontSize = "10px";
    btn2.style.textAlign = "center";
    btn2.style.inlineHeight = "25px";
    btn2.style.color = "black";
    btn2.style.backgroundcolor = "white";
    let sign = 1;
   /* btn2.onmouseup = function () {
        if (sign === 1) {
            myBottom();
        } else if (sign === -1) {
            thisBottom();
        }
    }*/
    btn2.onclick = function () {
        if (sign) {
            bottom_box.animate({
                transform:'translate(0,87%)'
            },{
                duration:1500,
                easing:'linear',
                iterations:1,
                fill:"forwards"
            })
            btn2.innerHTML="︿";
        }
        if(sign===-1){
            bottom_box.animate({
                    transform:'translate(0,0)'
                },{
                    duration:1500,
                    easing:'linear',
                    iterations:1,
                    fill:"forwards"
                })
            btn2.innerHTML="﹀";
            }
            sign*=-1;
    }
    /*function myBottom() {
        if (bottom) {
            bottom.style.height = "1%";
            sign *= -1;
        }
    }*/
    function myBottom() {
        if (bottom) {
         bottom.style.transform="translate(0,90%)";
            sign *= -1;
        }
    }

    function thisBottom() {
        bottom.style.height = "20%";
        sign *= -1;
    }
   /* function thisBottom() {
        bottom.style.transform="translate(0,0)";
